      "post": {
        "summary": "管理员登录",
        "deprecated": false,
        "description": "",
        "tags": [
          "管理端/认证"
        ],
        "parameters": [
          {
            "name": "Authorization",
            "in": "header",
            "description": "",
            "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJ7XCJpZFwiOlwiMVwiLFwidXNlcm5hbWVcIjpcImFkbWluXCIsXCJwaG9uZVwiOlwiMTM4MDAxMzgwMDBcIn0ifQ.wCEDKi31OQvskIs1IG84ScnQsIISCtNFkCS8c-lnB9k",
